{
  "@context": "https://project-open-data.cio.gov/v1.1/schema/catalog.jsonld",
  "@type": "dcat:Catalog",
  "conformsTo": "https://project-open-data.cio.gov/v1.1/schema",
  "dataset": [
    {
      "identifier": "bill-metadata",
      "title": "Bill Metadata",
      "description": "Bill metadata.json files from formatted snapshots",
      "keyword": ["legislation", "bills", "metadata"],
      "modified": "2024-11-25T00:00:00Z",
      "publisher": {
        "@type": "org:Organization",
        "name": "Windy Civi Project"
      },
      "contactPoint": {
        "@type": "vcard:Contact",
        "fn": "Chi Hack Night",
        "hasEmail": "mailto:info@chihacknight.org"
      },
      "accessLevel": "public",
      "license": "https://creativecommons.org/publicdomain/zero/1.0/",
      "x-filesystem-path": "{jurisdiction}/{session}/bills/{bill_id}-{title}/metadata.json",
      "x-schema-file": "schemas/metadata.schema.json",
      "x-path-parameters": [
        {
          "name": "jurisdiction",
          "in": "path",
          "required": true,
          "schema": {
            "type": "string",
            "pattern": "^[a-z]{2}$",
            "description": "Jurisdiction code (e.g., wy, il)"
          }
        },
        {
          "name": "session",
          "in": "path",
          "required": true,
          "schema": {
            "type": "string",
            "pattern": "^\\d{4}$",
            "description": "Legislative session year (e.g., 2024)"
          }
        },
        {
          "name": "bill_id",
          "in": "path",
          "required": true,
          "schema": {
            "type": "string",
            "pattern": "^[A-Z]{2,4}\\d+$",
            "description": "Bill identifier (e.g., SF0001, HB1234)"
          }
        },
        {
          "name": "title",
          "in": "path",
          "required": true,
          "schema": {
            "type": "string",
            "description": "URL-safe bill title"
          }
        }
      ]
    },
    {
      "identifier": "bill-action-logs",
      "title": "Bill Action Logs",
