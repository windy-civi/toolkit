{"legislative_session": "153", "identifier": "HB 202", "title": "AN ACT TO AMEND TITLE 21 REGARDING CIVIL PENALTIES FOR SPEEDING.", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": [], "abstracts": [{"note": "synopsis", "abstract": "This Act allows an additional assessment to be assessed for a speeding violation, proven through information captured on an electronic speed monitoring system, that exists in \u00a7 4101(j) of Title 11. This assessment was previously not permitted to be assessed unless the speeding occurred in a work zone.  The assessment is $10 and is paid to the Volunteer Ambulance Company Fund. \r\n\r\nIt also makes technical corrections to conform to the requirements of the Delaware Legislative Drafting Manual."}], "other_titles": [{"note": "short title", "title": "Vol. Ambulance Co. Fund/Assessment/Speeding"}], "other_identifiers": [], "actions": [{"description": "Introduced and Assigned to Public Safety & Homeland Security Committee in House", "date": "2025-06-05", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["introduction", "referral-committee"], "related_entities": [{"name": "Public Safety & Homeland Security", "entity_type": "organization", "organization_id": null}]}, {"description": "Reported Out of Committee (Public Safety & Homeland Security) in House with 1 Favorable, 4 On Its Merits", "date": "2025-06-17", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["committee-passage", "committee-passage-favorable"], "related_entities": [{"name": "Public Safety & Homeland Security", "entity_type": "organization", "organization_id": null}]}], "sponsorships": [{"name": "Madinah Wilson-Anton", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"chamber\": \"lower\", \"name\": \"Madinah Wilson-Anton\"}", "organization_id": null}, {"name": "Bryan Townsend", "classification": "primary", "entity_type": "person", "primary":