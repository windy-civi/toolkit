{"legislative_session": "153", "identifier": "HB 44", "title": "AN ACT TO AMEND TITLE 14 OF THE DELAWARE CODE RELATING TO MIGRANT EDUCATION.", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": [], "abstracts": [{"note": "synopsis", "abstract": "This Act requires the State to have a migrant education program to ensure that migrant children\u2019s educational needs are met. The program must provide the same services that were provided or were required to be provided under the Elementary and Secondary Education Act on January 19, 2025. If federal funding is insufficient to meet the needs of migratory education, the Delaware Department of Education must identify, transfer, or request the appropriation of supplemental funding from the State or other sources to ensure no interruption in services."}], "other_titles": [{"note": "short title", "title": "Migrant Education Program/Establish"}], "other_identifiers": [], "actions": [{"description": "Introduced and Assigned to Administration Committee in House", "date": "2025-03-06", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["introduction", "referral-committee"], "related_entities": [{"name": "Administration", "entity_type": "organization", "organization_id": null}]}, {"description": "Not Worked in Committee", "date": "2025-04-16", "organization_id": "~{\"classification\": \"lower\"}", "classification": [], "related_entities": []}, {"description": "Reported Out of Committee (Administration) in House with 4 Favorable, 1 On Its Merits", "date": "2025-05-07", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["committee-passage", "committee-passage-favorable"], "related_entities": [{"name": "Administration", "entity_type": "organization", "organization_id": null}]}], "sponsorships": [{"name": "Sean M. Lynn", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"chamber\": \"lower\", \"name\": \"Sean