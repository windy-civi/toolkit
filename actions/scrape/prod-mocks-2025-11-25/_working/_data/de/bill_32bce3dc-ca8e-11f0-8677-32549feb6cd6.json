{"legislative_session": "153", "identifier": "SB 25", "title": "AN ACT TO AMEND TITLES 14 AND 19 OF THE DELAWARE CODE RELATING TO THE PUBLIC SCHOOL EMPLOYMENT RELATIONS ACT.", "from_organization": "~{\"classification\": \"upper\"}", "classification": ["bill"], "subject": [], "abstracts": [{"note": "synopsis", "abstract": "This Act allows an employee organization to file a petition with the Board to become the exclusive representative of an appropriate bargaining unit for the purpose of collective bargaining. In the event an employee organization provides over 50% of the employees\u2019 approval, through their authorized signatures, then the Board may not order an election but must certify the employee organization.\r\n\r\nThis Act also makes technical corrections to confirm existing law to the standards of the Delaware Legislative Drafting Manual."}], "other_titles": [{"note": "short title", "title": "Public School Employment Relations Act/Petition/Exclusive Representative"}], "other_identifiers": [], "actions": [{"description": "Introduced and Assigned to Labor Committee in Senate", "date": "2025-04-17", "organization_id": "~{\"classification\": \"upper\"}", "classification": ["introduction", "referral-committee"], "related_entities": [{"name": "Labor", "entity_type": "organization", "organization_id": null}]}, {"description": "Reported Out of Committee (Labor) in Senate with 5 Favorable", "date": "2025-05-07", "organization_id": "~{\"classification\": \"upper\"}", "classification": ["committee-passage", "committee-passage-favorable"], "related_entities": [{"name": "Labor", "entity_type": "organization", "organization_id": null}]}, {"description": "Stricken in Senate", "date": "2025-05-15", "organization_id": "~{\"classification\": \"upper\"}", "classification": [], "related_entities": []}], "sponsorships": [{"name": "Bryan Townsend", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"chamber\": \"upper\", \"name\": \"Bryan Tow