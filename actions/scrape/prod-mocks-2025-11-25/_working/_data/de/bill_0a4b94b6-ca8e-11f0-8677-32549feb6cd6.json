{"legislative_session": "153", "identifier": "SB 77", "title": "AN ACT TO AMEND TITLE 30 OF THE DELAWARE CODE RELATING TO ELECTRONIC SMOKING DEVICES AND VAPOR PRODUCTS.", "from_organization": "~{\"classification\": \"upper\"}", "classification": ["bill"], "subject": [], "abstracts": [{"note": "synopsis", "abstract": "This Act prohibits the sale of electronic smoking devices and nicotine vapor products that are not included in a directory of manufacturers and products eligible to be sold in the state. To be added to the directory, manufacturers of electronic smoking devices and nicotine vapor products must certify that they have complied with federal tobacco product premarket application requirements and pay annual fees."}], "other_titles": [{"note": "short title", "title": "Electronic Smoking Devices/Nicotine Vapor Products/Prohibit/Not in Directory"}], "other_identifiers": [], "actions": [{"description": "Introduced and Assigned to Health & Social Services Committee in Senate", "date": "2025-03-20", "organization_id": "~{\"classification\": \"upper\"}", "classification": ["introduction", "referral-committee"], "related_entities": [{"name": "Health & Social Services", "entity_type": "organization", "organization_id": null}]}], "sponsorships": [{"name": "Laura V. Sturgeon", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"chamber\": \"upper\", \"name\": \"Laura V. Sturgeon\"}", "organization_id": null}, {"name": "Brian Pettyjohn", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"chamber\": \"upper\", \"name\": \"Brian Pettyjohn\"}", "organization_id": null}], "related_bills": [], "versions": [{"note": "Bill Text", "links": [{"url": "https://legis.delaware.gov/json/BillDetail/GenerateHtmlDocument?legislationId=141979&legislationTypeId=1&docTypeId=2&legislationName=SB77", "media_type": "text/html"}, {"url": "https://legis.delaware.gov/json/BillDetail/GeneratePdfDocument?legislationId=141979