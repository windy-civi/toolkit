{"legislative_session": "153", "identifier": "SB 103", "title": "AN ACT TO AMEND TITLE 29 OF THE DELAWARE CODE RELATING TO STATE PROCUREMENT.", "from_organization": "~{\"classification\": \"upper\"}", "classification": ["bill"], "subject": [], "abstracts": [{"note": "synopsis", "abstract": "This Act requires that for every public works contract or aggregate of public works contracts relating to a project in excess of $500,000 for new construction (including painting and decorating) or $45,000 for alteration, repair, renovation, rehabilitation, demolition or reconstruction (including painting and decorating of buildings or works) and which is paid for, in whole or in part, with public funds, which requires or involves the employment of mechanics and/or laborers shall contain a provision requiring the payment of the prevailing wage."}], "other_titles": [{"note": "short title", "title": "Public Works/>$500,000/Prevailing Wage"}], "other_identifiers": [], "actions": [{"description": "Introduced and Assigned to Elections & Government Affairs Committee in Senate", "date": "2025-04-09", "organization_id": "~{\"classification\": \"upper\"}", "classification": ["introduction", "referral-committee"], "related_entities": [{"name": "Elections & Government Affairs", "entity_type": "organization", "organization_id": null}]}, {"description": "Stricken in Senate", "date": "2025-04-14", "organization_id": "~{\"classification\": \"upper\"}", "classification": [], "related_entities": []}], "sponsorships": [{"name": "John \"Jack\" Walsh", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"chamber\": \"upper\", \"name\": \"John \\\"Jack\\\" Walsh\"}", "organization_id": null}, {"name": "Edward S. Osienski", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"chamber\": \"lower\", \"name\": \"Edward S. Osienski\"}", "organization_id": null}, {"name": "Sarah McBride", "classification": "primary", "entity_type": "pe