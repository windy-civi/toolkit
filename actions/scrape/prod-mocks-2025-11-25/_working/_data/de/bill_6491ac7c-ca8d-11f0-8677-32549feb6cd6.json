{"legislative_session": "153", "identifier": "HB 12", "title": "AN ACT TO AMEND TITLE 14 OF THE DELAWARE CODE RELATING TO TEACHER ACADEMY SCHOLARSHIPS.", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": [], "abstracts": [{"note": "synopsis", "abstract": "This Act creates a scholarship for students who have completed a Delaware Teacher Academy and are enrolled in a Delaware Educator Preparation Program.  This scholarship supplements the Educator Support Scholarship and is intended to fill a $2500 funding gap for aspiring educators who are in their first year at an Educator Preparation Program. No more than 35 scholarships may be awarded.  The scholarship will be expanded based on interest and funding in subsequent school years and will continue as funding is available."}], "other_titles": [{"note": "short title", "title": "DE Teacher Academy/Create Scholarships"}], "other_identifiers": [], "actions": [{"description": "Introduced and Assigned to Education Committee in House", "date": "2025-01-22", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["introduction", "referral-committee"], "related_entities": [{"name": "Education", "entity_type": "organization", "organization_id": null}]}, {"description": "Reported Out of Committee (Education) in House with 2 Favorable, 11 On Its Merits", "date": "2025-01-29", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["committee-passage", "committee-passage-favorable"], "related_entities": [{"name": "Education", "entity_type": "organization", "organization_id": null}]}], "sponsorships": [{"name": "Kimberly Williams", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"chamber\": \"lower\", \"name\": \"Kimberly Williams\"}", "organization_id": null}, {"name": "Laura V. Sturgeon", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"chamber\": \"upper\", \"name\": \"La