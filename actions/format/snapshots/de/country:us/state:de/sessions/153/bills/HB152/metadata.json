  "subject": [],
  "abstracts": [
    {
      "note": "synopsis",
      "abstract": "This Act expands the criminal offense of impersonating a police officer, firefighter, emergency medical technician, paramedic, or fire officer to also prohibit impersonation of a federal officer.\r\n\r\nThis Act also makes technical corrections to conform to the Legislative Drafting Manual and creates a definition of \u201claw-enforcement official,\u201d which includes all of the state officers already explicitly referenced in the statute, as well as federal officers."
    }
  ],
  "other_titles": [
    {
      "note": "short title",
      "title": "Impersonating LE/Firefighter/EMT/Paramedic/Expand Offense"
    }
  ],
