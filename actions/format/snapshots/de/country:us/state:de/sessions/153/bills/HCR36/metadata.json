  "identifier": "HCR 36",
  "title": "RECOGNIZING APRIL 23RD TO APRIL 24TH, 2025 AS YOM HASHOAH.",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "concurrent resolution"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "synopsis",
      "abstract": "This Concurrent Resolution recognizes sundown on April 23rd, 2025, through nightfall on April 24th, 2025, as Yom HaShoah, and urges all residents to reflect on the lessons of the Holocaust, honor the memory of its victims, and strive to eradicate prejudice and injustice in all forms."
    }
  ],
