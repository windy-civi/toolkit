  "legislative_session": "153",
  "identifier": "HB 117",
  "title": "AN ACT TO AMEND TITLE 3 OF THE DELAWARE CODE RELATING TO AGRICULTURAL LEASES OF STATE-OWNED LAND.",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
