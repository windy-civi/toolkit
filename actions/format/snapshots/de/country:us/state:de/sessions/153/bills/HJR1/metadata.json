{
  "legislative_session": "153",
  "identifier": "HJR 1",
  "title": "ESTABLISHING A TASK FORCE TO REDUCE REGULATORY BURDENS UPON SMALL RESTAURANTS.",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "joint resolution"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "synopsis",
      "abstract": "This Joint Resolution establishes the Small Restaurant Regulatory Reform Task Force, established for the goal of reducing unnecessary burdens created by the regulations promulgated by the Department of Health and Social Services. By June 1, 2026, the commission is to identify the unnecessary burdens created by the regulations, concrete steps that can be taken to ease the burdens, and what state funding, if any, would be necessary to ease those burdens."
    }
  ],
