  "legislative_session": "153",
  "identifier": "HB 182",
  "title": "AN ACT TO AMEND TITLE 11 OF THE DELAWARE CODE RELATING TO LAW ENFORCEMENT AGENCY AGREEMENTS.",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "synopsis",
      "abstract": "This Act prohibits law-enforcement agencies from entering into agreements with federal immigration enforcement authorities to enforce immigration violations or share immigration enforcement related data.\r\nThis Act is not intended to prevent a law-enforcement agency from working with the federal government on other public safety efforts."
    }
  ],
  "other_titles": [
    {
      "note": "short title",
      "title": "LE Agency Agreements/Prohibit/Fed. Immigration Enforcement"
    }
  ],
