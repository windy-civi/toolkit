  "legislative_session": "153",
  "identifier": "HB 51",
  "title": "AN ACT TO AMEND TITLE 14 OF THE DELAWARE CODE RELATING TO THE DELAWARE EDUCATOR APPRENTICESHIP PROGRAM.",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
