  "abstracts": [
    {
      "note": "summary",
      "abstract": "Adds the property of the Pokanoket Management Group, Trustee of the Pokanoket Tribe Land Trust, to the list of property designated for appropriations in lieu of municipal property tax, would concurrently exempt such property from municipal property tax."
    }
