    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "summary",
      "abstract": "Requires payments by the corporation increase from a $3.00 tipping fee per vehicle to a $6.00 tipping fee per vehicle, unless exempt. This act would also require fees be paid to Johnston on an annual rather than a biannual basis."
    }
