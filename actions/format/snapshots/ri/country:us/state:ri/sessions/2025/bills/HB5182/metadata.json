  "identifier": "HB5182",
  "title": "AN ACT RELATING TO PROPERTY -- MAINTENANCE OF PRIVATE EASEMENTS AND RIGHTS-OF-WAY",
  "from_organization": "~{\"classification\": \"lower\"}",
